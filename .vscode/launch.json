{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch NestJS",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "start:debug"],
            "program": "${workspaceFolder}/src/main.ts",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": ["<node_internals>/**"]
        }
    ]
}
